CREATE TABLE requesters ( id char(14) PRIMARY KEY NOT NULL, name varchar(255) NOT NULL, type varchar(50) NOT NULL, allowed_purposes JSON NOT NULL, status varchar(20) DEFAULT 'active' CHECK (status IN ('active', 'suspended', 'revoked')) NOT NULL, merchant_id char(14), user_id char(14), created_at bigint, updated_at bigint, INDEX idx_created_at (created_at), INDEX idx_updated_at (updated_at) );